package ex01_Calculator;

public class Calculator {
	
	public int plus(int a, int b) {
		int result= a+b;
		return result;
	}
	public int minus(int a, int b) {
		int result = a-b;
		return result;
	}
	public double multiple(double x, double y) {
		double result = x*y;
		return result;
	}
	public double divide(double x, double y) {
		double result = x/y;
		return result;
	}
}
