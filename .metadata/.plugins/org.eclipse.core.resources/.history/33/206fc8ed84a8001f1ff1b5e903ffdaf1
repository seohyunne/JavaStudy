
public class Ex6_18 {

	public boolean isNumber(String str) {
		for(int i=0; i<str.length();i++ ) {
			if(Character.isDigit(str.charAt(i))) {
				return true;
			}
			else return false;
		}
	}
	public static void main(String[] args) {
		String str = "123";
		System.out.println(str +"은 숫자입니까? "+isNumber(str));
		
		str = "1234d";
		System.out.println(str +"은 숫자입니까? "+isNumber(str));
	}

}
